</script>
<script type="text/javascript">
/*导航顶部搜索显示隐藏*/
$(function() {
    $('.search_link a').click(function() {
        var topheight = $('.header').css('height');
        if (topheight == '0px') {
            $('.header').animate({
                height: '138px'
            });
        } else {
            $('.header').animate({
                height: '0px'
            });
        }
    });
    $('.close_button').click(function() {
        var topheight = $('.header').css('height');
        if (topheight == '0px') {
            $('.header').animate({
                height: '138px'
            });
        } else {
            $('.header').animate({
                height: '0px'
            });
        }
    });
    $('.close_button').click(function() {
        var topheight = $('.header').css('height');
        if (topheight == '0px') {
            $('.header').animate({
                height: '138px'
            });
        } else {
            $('.header').animate({
                height: '0px'
            });
        }
    });
    /*顶部搜索*/
    $('.online .top').click(function() {
        $('html,body').animate({
            scrollTop: '0'
        }, 800);
    });
    /*下拉网页导航条变换样式*/
    $(function() {
        /*首页和内页的焦点图高度不同，这里高度有修改if (scrollY > 680)*/
        $(window).scroll(function() {
            var scrollY = $(document).scrollTop(); // 获取垂直滚动的距离，即滚动了多少
            if (scrollY > 580) { //如果滚动距离大于680px则隐藏，否则删除隐藏类
                $('.nav-top').removeClass('navgations');
                $('.nav-top').addClass('navgations_two');
                $('.menu-top').removeClass('menu');
                $('.menu-top').addClass('menu_two');
                $('.logo img').attr('src', 'tpl/en_titanhouser/images/logo2.png');
            } else {
                $('.nav-top').removeClass('navgations_two');
                $('.nav-top').addClass('navgations');
                $('.menu-top').removeClass('menu_two');
                $('.menu-top').addClass('menu');
                $('.logo img').attr('src', 'tpl/en_titanhouser/images/logo.png');
            }

            /*首页文字变化，页面滚动到相应位置再开始变化*/
            if (scrollY == 300) {
                expernum();
            }
        });
    });

    /*首页plan 遮罩效果*/

    $('.home_plan_list li').hover(function(e) {
        $(this).find('.home_plan_img a').animate({
            opacity: '1'
        });
    }, function() {
        $(this).find('.home_plan_img a').animate({
            opacity: '0'
        });
    });
    /*Online*/
    $('.erweima').hover(function() {}, function() {
        $('.online_erweima').animate({
            width: '0px',
            opacity: '0'
        });
    });
    $('.phone').hover(function() {
        $('.online_phone_num').animate({
            width: '150px',
            opacity: '1'
        });
    }, function() {
        $('.online_phone_num').animate({
            width: '0px',
            opacity: '0'
        });
    });

    /*项目列表页 遮罩效果*/
    $('.pro_piclist li').hover(function() {
        $(this).find('.pro_piclist_img span').animate({
            top: '0px'
        });
    }, function() {
        $(this).find('.pro_piclist_img span').animate({
            top: '220px'
        });
    });

});
</script>

