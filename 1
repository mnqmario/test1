<script type="text/javascript">
    $(function() {

        $('.close_button').click(function() {
            var topheight = $('.header').css('height');
            if (topheight == '0px') {
                $('.header').animate({
                    height: '138px'
                });
            } else {
                $('.header').animate({
                    height: '0px'
                });
            }
        });
    });
</script><script src="tpl/en_titanhouser/js/jquery-1.11.3.min.js"></script>
