<script type="text/javascript" src="tpl/en_titanhouser/js/countup/countUp.js"></script>
<script>
function expernum() {
    var getnum_1 = $('#num_1').attr('title');
    var getnum_2 = $('#num_2').attr('title');
    var getnum_3 = $('#num_3').attr('title');
    var getnum_4 = $('#num_4').attr('title');
    var options = {
        useEasing: true,
        useGrouping: true,
        separator: ',',
        decimal: '.',
        prefix: '',
        suffix: ''
    };
    var num_1 = new CountUp("num_1", 0, getnum_1, 0, 2.5, options);
    num_1.start();
    var num_2 = new CountUp("num_2", 0, getnum_2, 0, 2.5, options);
    num_2.start();
    var num_3 = new CountUp("num_3", 0, getnum_3, 0, 2.5, options);
    num_3.start();
    var num_4 = new CountUp("num_4", 0, getnum_4, 0, 2.5, options);
    num_4.start();
}
</script>
